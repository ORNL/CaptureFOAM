FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      MEA+;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    "(inlet|wall|surface)"
    {
        type            zeroGradient;
    }

    outlet
    {
        type            fixedValue;
        value           $internalField;
    }
    
    "(left|right)"
    {
        type            cyclic;
    }
}


// ************************************************************************* //
